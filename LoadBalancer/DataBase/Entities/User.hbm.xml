<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="LoadBalancer">
    <class name="LoadBalancer.DataBase.Entities.User" table="users">
        <id name="Id" unsaved-value="none">
            <generator class="identity"/>
        </id>
        <property name="Name"/>
        <property name="Email"/>
        <property name="Sex"/>
    </class>
</hibernate-mapping>